# river = {'yaroslavl': 'volga', 'SPB': 'neva'}
# for key, value in river.items():
#     print('This is river ' + value.title() + ' les flux ' + 'in ' + key.title())

favorite_languages = {
'jen': 'python',
'sarah': 'c',
'edward': 'ruby',
'phil': 'python',
}
users = ['jen']
for key, value in favorite_languages.items():
    if key in users:
        print(key + ' Благодарим за участи в опросе!' + '\n')
    elif key not in users:
        print(key + ' Прошу вас поучаствовать в опросе!' + '\n')
        users.insert(0, key)
print(users)
